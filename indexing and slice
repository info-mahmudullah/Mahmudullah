#slicing = create a substring by extracting elements from another  string
#   indexing [] or slice 9()
#      [start:stop:step]

name = "Mahmud ullah"

first_name = name[:6]
last_name = name[7:]
full_name = name[::5]
reverse_name = name[ :: -1]

print(first_name)
print(last_name)
print(full_name)
print(reverse_name)

website1 = "http://google.com"
website2 ="http://facebook.com"

slice = slice(7,-4)
print(website1[slice])
print(website2[slice])
